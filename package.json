{
	"name": "blog.w0s.jp",
	"version": "1.0.0",
	"description": "blog.w0s.jp",
	"homepage": "https://github.com/SaekiTominaga/blog.w0s.jp#readme",
	"bugs": {
		"url": "https://github.com/SaekiTominaga/blog.w0s.jp/issues"
	},
	"license": "MIT",
	"author": "Saeki Tominaga",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SaekiTominaga/blog.w0s.jp.git"
	},
	"scripts": {
		"app:dev": "nodemon --inspect node/dist/app.js",
		"build": "tsc --project node/tsconfig.json",
		"build:watch": "tsc --project node/tsconfig.json -w",
		"config:tsconvert": "json2ts -i node/configure/schema -o node/configure/type",
		"config:validate:common": "ajv --spec=draft7 -s node/configure/schema/common.json -d node/configure/common.json",
		"config:validate:list": "ajv --spec=draft7 -s node/configure/schema/list.json -d node/configure/list.json",
		"config:validate:message-preview": "ajv --spec=draft7 -s node/configure/schema/message-preview.json -d node/configure/message-preview.json",
		"config:validate:entry": "ajv --spec=draft7 -s node/configure/schema/entry.json -d node/configure/entry.json",
		"config:validate:category": "ajv --spec=draft7 -s node/configure/schema/category.json -d node/configure/category.json",
		"config:validate:twitter": "ajv --spec=draft7 -s node/configure/schema/twitter.json -d node/configure/twitter.json",
		"config:validate:paapi": "ajv --spec=draft7 -s node/configure/schema/paapi.json -d node/configure/paapi.json",
		"config:validate:feed-create": "ajv --spec=draft7 -s node/configure/schema/feed-create.json -d node/configure/feed-create.json",
		"config:validate:sitemap-create": "ajv --spec=draft7 -s node/configure/schema/sitemap-create.json -d node/configure/sitemap-create.json",
		"ejs:markuplint": "markuplint views/*.ejs",
		"_css:build-watch": "postcss public/style/_src/*.css --base public/style/_src -d public/style -w",
		"_css:compress": "onchange -f add -f change -v public/style/*.css -- brotlin compress {{file}} -m 1",
		"css": "run-p _css:*",
		"_js:build-watch": "webpack -w",
		"_js:compress": "onchange -f add -f change -v public/script/*.js public/script/*.mjs -- brotlin compress {{file}} -m 1",
		"js": "run-p _js:*",
		"_svg-watch:build": "onchange -f add -f change -v image/**/*.svg -- svgo -i {{file}} -o public/{{file}} --config=svgo.config.cjs",
		"_svg-watch:compress": "onchange -f add -f change -v public/image/**/*.svg -- brotlin compress {{file}} -m 1",
		"svg-watch": "run-p _svg-watch:*",
		"_svg-all:build": "svgo -f image -o public/image --config=svgo.config.cjs",
		"_svg-all:build-icon": "svgo -f image/icon -o public/image/icon --config=svgo.config.cjs",
		"_svg-all:build-sidebar": "svgo -f image/sidebar -o public/image/sidebar --config=svgo.config.cjs",
		"_svg-all:build-entry": "svgo -f image/entry -o public/image/entry --config=svgo.config.cjs",
		"_svg-all:favicon": "move-file public/image/favicon.svg public/favicon.svg",
		"_svg-all:compress": "brotlin compress public/**/*.svg -m 1",
		"svg-all": "run-s _svg-all:*",
		"test": "jest --coverage"
	},
	"dependencies": {
		"@log4js-node/smtp": "^1.1.0",
		"@saekitominaga/paapi-item-image-url-parser": "^2.1.2",
		"@saekitominaga/string-escape-html": "^1.0.2",
		"amazon-paapi": "^1.0.6",
		"basic-auth": "^2.0.1",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dayjs": "^1.10.7",
		"ejs": "^3.1.6",
		"express": "^4.17.1",
		"highlight.js": "^11.3.1",
		"htpasswd-js": "^1.0.2",
		"jsdom": "^18.1.0",
		"log4js": "^6.3.0",
		"md5": "^2.3.0",
		"sqlite": "^4.0.23",
		"sqlite3": "^5.0.2",
		"twitter-v2": "^1.1.0",
		"w3c-xmlserializer": "^3.0.0",
		"xml-formatter": "^2.5.1"
	},
	"devDependencies": {
		"@markuplint/ejs-parser": "*",
		"@types/basic-auth": "^1.1.3",
		"@types/compression": "^1.7.2",
		"@types/cors": "^2.8.12",
		"@types/ejs": "^3.1.0",
		"@types/express": "^4.17.13",
		"@types/jest": "^27.0.2",
		"@types/jsdom": "^16.2.13",
		"@types/md5": "^2.3.1",
		"@types/trusted-types": "^2.0.2",
		"@types/w3c-xmlserializer": "^2.0.2",
		"@typescript-eslint/eslint-plugin": "^5.3.1",
		"@typescript-eslint/parser": "^5.3.1",
		"ajv-cli": "^5.0.0",
		"brotlin": "^1.1.0",
		"console-locale-timestamp": "^1.0.12",
		"cssnano": "^5.0.10",
		"eslint": "^8.2.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-jest": "^25.2.4",
		"eslint-plugin-jsdoc": "^37.0.3",
		"jest": "^27.3.1",
		"json-schema-to-typescript": "^10.1.5",
		"markuplint": "^1.11.4",
		"move-file-cli": "^3.0.0",
		"nodemon": "^2.0.15",
		"npm-run-all": "^4.1.5",
		"onchange": "^7.1.0",
		"paapi5-typescript-sdk": "^0.2.0",
		"postcss": "^8.3.11",
		"postcss-cli": "^9.0.2",
		"postcss-custom-media": "^8.0.0",
		"postcss-import": "^14.0.2",
		"postcss-nesting": "^9.0.0",
		"prettier": "^2.4.1",
		"stylelint": "^14.1.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-standard": "^24.0.0",
		"stylelint-prettier": "^2.0.0",
		"svgo": "^2.8.0",
		"ts-jest": "^27.0.7",
		"ts-loader": "^9.2.6",
		"typescript": "^4.4.4",
		"webpack": "^5.64.0",
		"webpack-cli": "^4.9.1"
	}
}
