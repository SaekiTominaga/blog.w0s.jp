{
	"name": "blog.w0s.jp",
	"version": "1.0.0",
	"description": "blog.w0s.jp",
	"homepage": "https://github.com/SaekiTominaga/blog.w0s.jp#readme",
	"bugs": {
		"url": "https://github.com/SaekiTominaga/blog.w0s.jp/issues"
	},
	"license": "MIT",
	"author": "Saeki Tominaga",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SaekiTominaga/blog.w0s.jp.git"
	},
	"scripts": {
		"app:dev": "nodemon node/dist/app.js",
		"build": "tsc --project node/tsconfig.json",
		"build:watch": "tsc --project node/tsconfig.json -w",
		"config:tsconvert": "json2ts -i node/configure/schema -o node/configure/type",
		"config:validate:common": "ajv --spec=draft7 -s node/configure/schema/common.json -d node/configure/common.json",
		"config:validate:list": "ajv --spec=draft7 -s node/configure/schema/list.json -d node/configure/list.json",
		"config:validate:topic": "ajv --spec=draft7 -s node/configure/schema/topic.json -d node/configure/topic.json",
		"config:validate:category": "ajv --spec=draft7 -s node/configure/schema/category.json -d node/configure/category.json",
		"config:validate:twitter": "ajv --spec=draft7 -s node/configure/schema/twitter.json -d node/configure/twitter.json",
		"config:validate:paapi": "ajv --spec=draft7 -s node/configure/schema/paapi.json -d node/configure/paapi.json",
		"config:validate:feed-create": "ajv --spec=draft7 -s node/configure/schema/feed-create.json -d node/configure/feed-create.json",
		"_css:build-watch": "postcss docs/style/_src/*.css --base docs/style/_src -d docs/style -w",
		"_css:compress": "onchange -f add -f change -v docs/style/*.css -- brotlin compress {{file}} -m 1",
		"css": "run-p _css:*",
		"_js:build-watch": "webpack -w",
		"_js:compress": "onchange -f add -f change -v docs/script/*.js docs/script/*.mjs -- brotlin compress {{file}} -m 1",
		"js": "run-p _js:*",
		"_svg-watch:build": "onchange -f add -f change -v image/**/*.svg -- svgo -i {{file}} -o docs/{{file}} --config=svgo.config.cjs",
		"_svg-watch:compress": "onchange -f add -f change -v docs/image/**/*.svg -- brotlin compress {{file}} -m 1",
		"svg-watch": "run-p _svg-watch:*",
		"_svg-all:build": "svgo -f image -o docs/image --config=svgo.config.cjs",
		"_svg-all:build-icon": "svgo -f image/icon -o docs/image/icon --config=svgo.config.cjs",
		"_svg-all:build-sidebar": "svgo -f image/sidebar -o docs/image/sidebar --config=svgo.config.cjs",
		"_svg-all:build-topic": "svgo -f image/topic -o docs/image/topic --config=svgo.config.cjs",
		"_svg-all:favicon": "move-file docs/image/favicon.svg docs/favicon.svg",
		"_svg-all:compress": "brotlin compress docs/**/*.svg -m 1",
		"svg-all": "run-s _svg-all:*"
	},
	"dependencies": {
		"@log4js-node/smtp": "^1.1.0",
		"@saekitominaga/paapi-item-image-url-parser": "^2.1.1",
		"@saekitominaga/string-escape-html": "^1.0.1",
		"amazon-paapi": "^1.0.5",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dayjs": "^1.10.4",
		"ejs": "^3.1.6",
		"express": "^4.17.1",
		"highlight.js": "^10.7.2",
		"jsdom": "^16.5.3",
		"log4js": "^6.3.0",
		"md5": "^2.3.0",
		"sqlite": "^4.0.21",
		"sqlite3": "^5.0.2",
		"twitter-v2": "^1.1.0",
		"w3c-xmlserializer": "^2.0.0"
	},
	"devDependencies": {
		"@types/compression": "^1.7.0",
		"@types/cors": "^2.8.10",
		"@types/ejs": "^3.0.6",
		"@types/express": "^4.17.11",
		"@types/jsdom": "^16.2.10",
		"@types/md5": "^2.3.0",
		"@types/trusted-types": "^2.0.0",
		"@types/w3c-xmlserializer": "^2.0.1",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"@typescript-eslint/parser": "^4.22.0",
		"ajv-cli": "^5.0.0",
		"brotlin": "^1.1.0",
		"console-locale-timestamp": "^1.0.12",
		"cssnano": "^5.0.1",
		"eslint": "^7.25.0",
		"eslint-config-prettier": "^8.2.0",
		"eslint-plugin-jsdoc": "^32.3.1",
		"json-schema-to-typescript": "^10.1.4",
		"move-file-cli": "^2.0.0",
		"nodemon": "^2.0.7",
		"npm-run-all": "^4.1.5",
		"onchange": "^7.1.0",
		"paapi5-typescript-sdk": "^0.2.0",
		"postcss": "^8.2.12",
		"postcss-cli": "^8.3.1",
		"postcss-custom-media": "^8.0.0",
		"postcss-import": "^14.0.1",
		"postcss-nesting": "^7.0.1",
		"prettier": "^2.2.1",
		"stylelint": "^13.12.0",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-config-standard": "^21.0.0",
		"stylelint-prettier": "^1.2.0",
		"svgo": "^2.3.0",
		"ts-loader": "^9.0.0",
		"typescript": "^4.2.4",
		"webpack": "^5.35.1",
		"webpack-cli": "^4.6.0"
	}
}
