{
	"extends": "../tsconfig.json",
	"include": ["@types/*.d.ts", "src/**/*.ts"],
	"compilerOptions": {
		/* Type Checking */
		"noUnusedParameters": false,

		/* Modules */
		"module": "es2022",

		/* Emit */
		"outDir": "dist",

		/* Interop Constraints */
		"esModuleInterop": true,

		/* Language and Environment */
		"target": "ESNext"
	}
}
