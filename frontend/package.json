{
	"name": "frontend",
	"private": true,
	"type": "module",
	"scripts": {
		"predev": "rimraf -g \"../public/**/*.svg.br\" \"../public/{script,style}/*.br\"",
		"dev": "pnpm run /^dev:.*/",
		"dev:css": "postcss style/*.css --base style -d ../public/style -w",
		"dev:js": "rollup -c -w --environment build:development",
		"dev:svg": "node build/svg.ts -i image -o ../public -c svgo.config.js -w",
		"prebuild": "pnpm -F frontend lint && rimraf -g \"../public/**/*.{svg,svg.br}\" \"../public/{script,style}/*\"",
		"build": "pnpm run /^build:.*/",
		"build:css": "postcss style/*.css --base style -d ../public/style",
		"build:js": "rollup -c --environment build:production",
		"build:svg": "node build/svg.ts -i image -o ../public -c svgo.config.js",
		"postbuild": "prettier --config ../.prettierrc.mjs -w \"../public/script/*.{js,mjs}\" \"../public/style/*.css\" && brotli-cli compress -g \"../public/script/*.{js,mjs}\" \"../public/style/*.css\" \"../public/**/*.svg\" -m text",
		"lint": "pnpm run /^lint:.*/",
		"lint:ts": "tsc --project script --noEmit",
		"lint:js": "eslint build/**/*.ts script/**/*.ts",
		"lint:css": "stylelint style/**/*.css"
	},
	"dependencies": {
		"@w0s/base.css": "1.3.3",
		"@w0s/button-clipboard": "3.0.4",
		"@w0s/footnote-reference-popover": "7.3.3",
		"@w0s/form-before-unload-confirm": "3.0.3",
		"@w0s/form-submit-overlay": "4.0.4",
		"@w0s/input-file-preview": "3.0.6",
		"@w0s/paapi-item-image-url-parser": "3.2.3",
		"@w0s/report-js-error": "4.0.1",
		"@w0s/report-same-referrer": "5.0.1",
		"@w0s/string-convert": "3.0.3",
		"@w0s/tab": "5.0.2",
		"chokidar": "5.0.0",
		"slash": "5.1.0",
		"svgo": "4.0.0"
	},
	"devDependencies": {
		"@csstools/postcss-global-data": "4.0.0",
		"@rollup/plugin-commonjs": "29.0.0",
		"@rollup/plugin-node-resolve": "16.0.3",
		"@rollup/plugin-typescript": "12.3.0",
		"@types/trusted-types": "2.0.7",
		"brotli-cli": "2.1.1",
		"postcss": "8.5.6",
		"postcss-cli": "11.0.1",
		"postcss-custom-media": "12.0.0",
		"postcss-discard-comments": "7.0.5",
		"postcss-discard-empty": "7.0.1",
		"postcss-import": "16.1.1",
		"postcss-nesting": "14.0.0",
		"rollup": "4.57.1",
		"vfile-message": "4.0.3"
	},
	"packageManager": "pnpm@10.28.1"
}
