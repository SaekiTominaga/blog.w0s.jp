{
	"name": "@blog.w0s.jp/frontend",
	"private": true,
	"type": "module",
	"scripts": {
		"predev": "rimraf -g \"../public/**/*.svg.br\" \"../public/{script,style}/*.br\"",
		"dev": "conc \"postcss style/*.css --base style -d ../public/style -w\" \"rollup -c -w --environment build:development\" \"node build/svg.ts -i image -o ../public -c svgo.config.js -w\"",
		"prebuild": "npm run lint -w @blog.w0s.jp/frontend && rimraf -g \"../public/**/*.{svg,svg.br}\" \"../public/{script,style}/*\"",
		"build": "conc \"postcss style/*.css --base style -d ../public/style\" \"rollup -c --environment build:production\" \"node build/svg.ts -i image -o ../public -c svgo.config.js\"",
		"postbuild": "prettier --config ../.prettierrc.mjs -w \"../public/script/*.{js,mjs}\" \"../public/style/*.css\" && brotli-cli compress -g \"../public/script/*.{js,mjs}\" \"../public/style/*.css\" \"../public/**/*.svg\" -m text",
		"lint": "conc \"tsc --project script --noEmit\" \"eslint build/**/*.ts script/**/*.ts\" \"stylelint style/**/*.css\""
	},
	"dependencies": {
		"@w0s/button-clipboard": "^3.0.3",
		"@w0s/footnote-reference-popover": "^7.3.0",
		"@w0s/form-before-unload-confirm": "^3.0.2",
		"@w0s/form-submit-overlay": "^4.0.3",
		"@w0s/input-file-preview": "^3.0.5",
		"@w0s/paapi-item-image-url-parser": "^3.2.3",
		"@w0s/report-js-error": "^4.0.1",
		"@w0s/report-same-referrer": "^5.0.0",
		"@w0s/string-convert": "^3.0.3",
		"@w0s/tab": "^4.4.2",
		"chokidar": "^5.0.0",
		"slash": "^5.1.0",
		"svgo": "^4.0.0"
	},
	"devDependencies": {
		"@csstools/postcss-global-data": "^3.0.0",
		"@rollup/plugin-commonjs": "^29.0.0",
		"@rollup/plugin-node-resolve": "^16.0.3",
		"@rollup/plugin-typescript": "^12.3.0",
		"@types/trusted-types": "^2.0.7",
		"brotli-cli": "^2.1.0",
		"postcss": "^8.5.1",
		"postcss-cli": "^11.0.0",
		"postcss-custom-media": "^11.0.5",
		"postcss-discard-comments": "^7.0.5",
		"postcss-discard-empty": "^7.0.1",
		"postcss-import": "^16.1.0",
		"postcss-nesting": "^13.0.1",
		"rollup": "^4.53.5",
		"vfile-message": "^4.0.3"
	}
}
