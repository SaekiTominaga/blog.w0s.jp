<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<url>
		<loc>https://blog.w0s.jp/</loc>
		<lastmod><%= updatedAt.format('YYYY-MM-DDTHH:mm:ssZ') %></lastmod>
	</url>
	<%_ for (const entry of entries) { _%>
	<url>
		<loc>https://blog.w0s.jp/entry/<%= entry.id %></loc>
		<lastmod><%= entry.updatedAt.format('YYYY-MM-DDTHH:mm:ssZ') %></lastmod>
	</url>
	<%_ } _%>
</urlset>
