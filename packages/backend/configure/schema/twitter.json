{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://blog.w0s.jp/twitter.json",
	"type": "object",
	"title": "Twitter API",
	"required": ["development", "production"],
	"properties": {
		"development": {
			"type": "object",
			"title": "Development",
			"$ref": "#/definitions/app_keys"
		},
		"production": {
			"type": "object",
			"title": "Production",
			"$ref": "#/definitions/app_keys"
		},
		"additionalProperties": false
	},
	"additionalProperties": false,
	"definitions": {
		"app_keys": {
			"type": "object",
			"required": ["bearer_token", "consumer_key", "consumer_secret", "access_token", "access_token_secret"],
			"properties": {
				"bearer_token": {
					"type": "string",
					"title": "Bearer Token"
				},
				"consumer_key": {
					"type": "string",
					"title": "API key"
				},
				"consumer_secret": {
					"type": "string",
					"title": "API key secret"
				},
				"access_token": {
					"type": "string",
					"title": "Access token"
				},
				"access_token_secret": {
					"type": "string",
					"title": "Access token secret"
				}
			},
			"additionalProperties": false
		}
	}
}
